            "id": 11,
            "editable": true,
            "title": "",
            "transparent": true,
            "error": false,
            "type": "text"
          }
        ]
      },
      {
        "title": "Feeder Elements",
        "collapse": false,
        "editable": true,
        "height": "150px",
        "panels": [
          {
            "type": "graph",
            "pointradius": 5,
            "percentage": false,
            "decimals": 0,
            "targets": [
              {
                "textEditor": false,
                "target": "aliasByMetric(aliasSub(carbon-writer.$host.FEEDER_PREV.ProactiveEngine.feeder.entries.diff, '(.*)-', ''))",
                "refId": "A"
              },
              {
                "hide": true,
                "textEditor": false,
                "target": "aliasByMetric(aliasSub(carbon-writer.$host.FEEDER_PREV.ProactiveEngine.feeder.entries.current, '(.*)-', ''))",
                "refId": "B"
              },
              {
                "refId": "C",
                "target": "aliasByMetric(aliasSub(carbon-writer.$host.FEEDER_PREV.ProactiveEngine.feeder.entries.max, '(.*)-', ''))",
                "textEditor": false,
                "hide": true
              },
              {
                "target": "aliasByMetric(aliasSub(carbon-writer.$host.FEEDER_PREV.ProactiveEngine.feeder.invalidations, '(.*)-', ''))",
                "refId": "D",
                "textEditor": false,
                "hide": true
              },
              {
                "textEditor": false,
                "target": "aliasByMetric(aliasSub(carbon-writer.$host.FEEDER_PREV.ProactiveEngine.feeder.heartbeat, '(.*)-', ''))",
                "refId": "E"
              }
            ],
            "editable": true,
            "grid": {
              "threshold2": null,
              "threshold1": null,
              "threshold1Color": "rgba(216, 200, 27, 0.27)",
              "threshold2Color": "rgba(234, 112, 112, 0.22)"
            },
            "steppedLine": false,
            "links": [],
            "isNew": true,
            "seriesOverrides": [
              {
                "alias": "heartbeat",
                "yaxis": 2
              }
            ],
            "nullPointMode": "connected",
            "linewidth": 1,
            "aliasColors": {},
            "error": false,
            "lines": true,
            "timeFrom": null,
            "title": "Feeder Elements",
            "id": 12,
            "renderer": "flot",
            "xaxis": {
              "show": true
            },
            "fill": 3,
            "yaxes": [
              {
                "logBase": 1,
                "format": "none",
                "min": 0,
                "show": true,
                "max": null
              },
              {
                "min": 0,
                "show": true,
                "logBase": 1,
                "format": "ms",
                "max": 60000
              }
            ],
            "datasource": "graphite",
            "points": false,
            "tooltip": {
              "sort": 0,
              "msResolution": false,
              "value_type": "cumulative",
              "ordering": "alphabetical",
              "shared": true
            },
            "span": 12,
            "legend": {
              "rightSide": true,
              "total": false,
              "min": false,
              "max": true,
              "alignAsTable": true,
              "avg": false,
              "show": true,
              "current": true,
              "values": true
            },
            "bars": false,
            "timeShift": null,
            "stack": false
          }
        ]
      },
      {
        "panels": [
          {
            "linewidth": 1,
            "lines": true,
            "aliasColors": {},
            "error": false,
            "title": "UAPI Cache",
            "timeFrom": null,
            "xaxis": {
              "show": true
            },
            "id": 13,
            "renderer": "flot",
            "yaxes": [
              {
                "label": null,
                "max": null,
                "min": 0,
                "show": true,
                "logBase": 1,
