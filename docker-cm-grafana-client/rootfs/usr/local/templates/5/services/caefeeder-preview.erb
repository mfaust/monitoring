            "datasource": "graphite",
            "fill": 3,
            "span": 12,
            "tooltip": {
              "msResolution": false,
              "sort": 0,
              "value_type": "cumulative",
              "ordering": "alphabetical",
              "shared": true
            },
            "legend": {
              "max": true,
              "min": false,
              "total": false,
              "rightSide": true,
              "values": true,
              "current": true,
              "show": true,
              "avg": false,
              "alignAsTable": true
            },
            "points": false,
            "bars": false,
            "timeShift": null,
            "stack": false,
            "percentage": false,
            "pointradius": 5,
            "type": "graph",
            "decimals": 1,
            "targets": [
              {
                "textEditor": false,
                "refId": "A",
                "target": "alias(aliasSub(carbon-writer.$host.FEEDER_PREV.CapConnection.heap.cache.size, '(.*)-', ''), 'max')"
              },
              {
                "textEditor": false,
                "target": "alias(aliasSub(carbon-writer.$host.FEEDER_PREV.CapConnection.heap.cache.used, '(.*)-', ''), 'used')",
                "refId": "B"
              },
              {
                "refId": "C",
                "target": "alias(aliasSub(carbon-writer.$host.FEEDER_PREV.CapConnection.heap.cache.used_percent, '(.*)-', ''), 'percent')",
                "textEditor": false
              }
            ],
            "editable": true,
            "grid": {
              "threshold1Color": "rgba(216, 200, 27, 0.27)",
              "threshold2Color": "rgba(234, 112, 112, 0.22)",
              "threshold2": null,
              "threshold1": null
            },
            "links": [],
            "steppedLine": false,
            "seriesOverrides": [
              {
                "yaxis": 2,
                "alias": "percent",
                "color": "#BF1B00"
              }
            ],
            "isNew": true,
            "nullPointMode": "connected"
          }
        ],
        "height": "150px",
        "editable": true,
        "title": "UAPI Cache",
        "collapse": false
      }
