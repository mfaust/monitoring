    "templating": {
      "list": [
        {
          "current": {
            "text": "<%= slug %>",
            "value": "<%= graphite_identifier %>"
          },
          "options": [
            {
              "value": "<%= graphite_identifier %>",
              "text": "<%= slug %>"
            }
          ],
          "hide": 2,
          "label": null,
          "name": "host",
          "query": "<%= graphite_identifier %>",
          "type": "constant"
        },
        {
          "name": "mls",
          "label": null,
          "type": "constant",
          "query": "<%= graphite_identifier %>",
          "hide": 2,
          "current": {
            "value": "<%= graphite_identifier %>",
            "text": "<%= slug %>"
          },
          "options": [
            {
              "text": "<%= slug %>",
              "value": "<%= graphite_identifier %>"
            }
          ]
        }
      ]
