                "min": 0,
                "logBase": 1,
                "max": null
              }
            ],
            "pointradius": 5,
            "decimals": 0,
            "xaxis": {
              "show": true
            },
            "targets": [
              {
                "refId": "A",
                "target": "alias(carbon-writer.$host.RLS.QueryPool.queries.max, 'queries max')",
                "textEditor": false
              },
              {
                "textEditor": false,
                "refId": "B",
                "target": "alias(carbon-writer.$host.RLS.QueryPool.queries.waiting, 'queries waiting')"
              },
              {
                "refId": "C",
                "target": "alias(carbon-writer.$host.RLS.QueryPool.executors.idle, 'executors idle')",
                "textEditor": false
              },
              {
                "textEditor": false,
                "target": "alias(carbon-writer.$host.RLS.QueryPool.executors.running, 'executors running')",
                "refId": "D"
              }
            ],
            "lines": true,
            "stack": false,
            "timeFrom": null,
            "timeShift": null,
            "percentage": false,
            "grid": {
              "threshold2": null,
              "thresholdLine": false,
              "threshold1Color": "rgba(216, 200, 27, 0.27)",
              "threshold2Color": "rgba(234, 112, 112, 0.22)",
              "threshold1": null
            },
            "steppedLine": false,
            "title": "Query Pool",
            "isNew": true,
            "bars": false,
            "type": "graph",
            "datasource": "graphite",
            "tooltip": {
              "sort": 0,
              "msResolution": false,
              "ordering": "alphabetical",
              "value_type": "cumulative",
              "shared": true
