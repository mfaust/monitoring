            "legend": {
              "max": true,
              "total": false,
              "current": true,
              "show": true,
              "rightSide": true,
              "min": false,
              "alignAsTable": true,
              "avg": false,
              "values": true
            },
            "datasource": "graphite",
            "type": "graph",
            "bars": false,
            "percentage": false,
            "title": "diff Sequence Numbers between MLS & RLS",
            "steppedLine": false,
            "xaxis": {
              "show": true,
              "values": [],
              "mode": "time",
              "name": null
            },
            "lines": true,
            "targets": [
              {
                "target": "alias(carbon-writer.$mls.MLS.Server.Repository.SequenceNumber, 'seq MLS')",
                "refId": "A",
                "hide": true
              },
              {
                "hide": true,
