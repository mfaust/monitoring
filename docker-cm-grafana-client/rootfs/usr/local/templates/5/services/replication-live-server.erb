          }
        ],
        "height": "25px",
        "editable": true
      },
      {
        "height": "170px",
        "editable": true,
        "panels": [
          {
            "yaxes": [
              {
                "format": "none",
                "logBase": 1,
                "max": null,
                "show": true,
                "min": 0
              },
              {
                "format": "short",
